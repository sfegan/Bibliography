@ARTICLE{REF::LUBANDORZHIEV::PE_BACKSCATTERING,
   title = "Photoelectron backscattering in vacuum phototubes ",
 journal = {\nima},
  volume = "567",
  number = "1",
   pages = "12 - 16",
    year = "2006",
    note = "Proceedings of the 4th International Conference on New Developments in Photodetection (BEAUNE)",
    issn = "0168-9002",
